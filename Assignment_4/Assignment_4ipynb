      "cell_type": "markdown",
      "metadata": {
        "id": "CflEyloeP_sw"
      },
      "source": [
        "## Handle Glitches for Battery-Less problem"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "qBIXcObdQSca"
      },
      "outputs": [],
      "source": [
        "Ebatt_max = 0"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "GMvZS1qlQScb",
        "outputId": "3b8eb639-4b6a-4369-d43e-7c08339ad004"
      },
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "total fuel consumption of the vehicle - battery-less car - epsilon-modified objective: 5896.809021680713\n"
          ]
        }
      ],
      "source": [
        "#variables\n",
        "Peng = cp.Variable(T) #\"Power produced by combustion engine\"\n",
        "Pmg = cp.Variable(T) #\"Power produced by the motor/generator\"\n",
        "Pbr = cp.Variable(T) #\"Power absorbed by the friction break\"\n",
        "E = cp.Variable(T+1) #\"Energy stored in the battery\"\n",
        "\n",
        "\n",
        "#constraints\n",
        "constraints = []\n",
        "\n",
        "\n",
        "constraints += [E[T] == E[0]]\n",
        "                \n",
        "tot_fuel_consumption = 0\n",
        "epsilon = 0.0001\n",
        "for i in range(T): \n",
        "  constraints += [Peng[i] + Pmg[i] - Pbr[i] == Preq[i],\n",
        "                  Peng[i] >= 0, Peng[i] <= Peng_max,\n",
        "                  Pmg[i] >= Pmg_min, Pmg[i] <= Pmg_max,\n",
        "                  Pbr>=0,\n",
        "                  E[i] >= 0, E[i] <= Ebatt_max,\n",
        "                  eta * Pmg[i] <= -E[i+1] + E[i] - Pmg[i],\n",
        "                  eta * Pmg[i] >= E[i+1] - E[i] + Pmg[i]]\n",
        "\n",
        "  #objective               \n",
        "  tot_fuel_consumption += Peng[i] + (cp.sum_squares(Peng[i])*gamma) + epsilon * cp.maximum(0,-Pmg[i])\n",
        "\n",
        "prob1 = cp.Problem(cp.Minimize(tot_fuel_consumption), constraints)\n",
        "res_no_batt_epsilon = prob1.solve(solver = cp.ECOS)\n",
        "print(f\"total fuel consumption of the vehicle - battery-less car - epsilon-modified objective: {res_no_batt_epsilon}\")"
      ]
    },
